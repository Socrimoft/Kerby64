{
    "entryPoints": [
        "src/**/*.ts",
        "src/**/*.json"
    ],
    "name": "Kerby64",
    "packageOptions": {
        "package": "package.json",
        "readme": "README.md"
    },
    "projectDocuments": ["README.md"],
    "exclude": [
        "**/node_modules/**",
        "**/dist/**",
        "**/build/**",
        "**/coverage/**",
        "**/test/**",
        "**/tests/**",
        "**/*.spec.ts",
        "**/*.test.ts"
    ],
    "alwaysCreateEntryPointModule": false,
    "blockTags": [
        "@defaultValue",
        "@deprecated",
        "@example",
        "@param",
        "@privateRemarks",
        "@remarks",
        "@returns",
        "@see",
        "@throws",
        "@extends",
        "@implements",
        "@license",
        "@return",
        "@satisfies",
        "@type",
        "@todo",
        "@method"
    ]
}